{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": ">=1.2",
    "webservices.rest": "^2.24.0"
  },
  "pages": [
   {
     "component": "billingAdminHome",
     "route":"rwanda-billing-admin"
   }
 ],
  "extensions": [
    {
      "component": "root",
      "name": "billing-root",
      "slot": "rwanda-billing-slot"
    },
    {
      "name": "billing-summary-dashboard-link",
      "component": "billingSummaryDashboardLink",
      "slot": "patient-chart-dashboard-slot",
      "order": 11,
      "meta": {
        "columns": 1,
        "columnSpan": 1,
        "slot": "patient-chart-billing-dashboard-slot",
        "path": "billing-listing"
      }
    },
    {
      "name": "billing-patient-summary",
      "component": "billingPatientSummary",
      "slot": "patient-chart-billing-dashboard-slot",
      "meta": {
        "columnSpan": 1
      }
    },
    {
      "name": "billing-patient-banner",
      "component": "billingPatientBanner",
      "slot": "patient-header-slot",
      "meta": {
        "columnSpan": 1
      }
    }
  ]
}
{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": ">=1.2",
    "webservices.rest": "^2.24.0"
  },
  "extensions": [
    {
      "component": "billingDashboardLink",
      "name": "billing-home-left-panel-link",
      "slot": "homepage-dashboard-slot",
      "meta": {
        "name": "billing",
        "title": "Billing",
        "slot": "billing-app-slot",
        "isFolder": true
      }
    },
    {
      "component": "root",
      "name": "billing-root",
      "slot": "billing-app-slot"
    },
    {
      "name": "department-link",
      "slot": "billing-app-slot",
      "component": "departmentLink",
      "meta": {
        "slot": "billing-submenu-slot",
        "path": "department"
      }
    },
    {
      "name": "service-link",
      "slot": "billing-app-slot",
      "component": "serviceLink",
      "meta": {
        "slot": "billing-submenu-slot",
        "path": "service"
      }
    }
  ]
}